<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@page import="com.whfp.oa.commons.util.ServletUtil"%>
<%@page import="com.whfp.oa.commons.model.Member"%>
<%@page import="com.whfp.oa.manager.hlkj.dict.bean.HlkjDict"%>
<%@ taglib uri="http://www.springframework.org/tags" prefix="s" %>
<%@ include file="/WEB-INF/jsp/commons/include.inc.jsp" %>
<%--
	模块：企业宣传--公告管理 --系统公告添加
--%>
 

 
<div style="width: 98%;margin: 0 auto ;" >
			
<form method="post" action="hlkj/zcgl/update.do"  onsubmit="return validateSubmitForm(this)">
	<table class="table table-bordered ">
			<tr>
				<th>资产名称：</th>
				<td  colspan="3">
				<input type="text" name="zcmc" value="${qq.zcmc }" class="easyui-validatebox" required="true" />
				</td>
			</tr>
			<tr>
				<th>资产编号：</th>
				<td colspan="3">
					<input type="text" name="zcbh" value="${qq.zcbh }" class="easyui-validatebox" value="0" required="true" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">出厂编号：</th>
				<td  colspan="3">
					<input type="text" name="ccbh" value="${qq.ccbh }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">单位：</th>
				<td  colspan="3">
					<input type="text" name="dept" value="${qq.dept }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">供应商：</th>
				<td  colspan="3">
					<input type="text" name="gys" value="${qq.gys }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">安装地点：</th>
				<td  colspan="3">
					<input type="text" name="azdd" value="${qq.azdd }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">采购时间：</th>
				<td  colspan="3">
					<input type="text" name="cgsj" value="${qq.cgsj }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">使用寿命：</th>
				<td  colspan="3">
					<input type="text" name="sysm" value="${qq.sysm }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">采购价格：</th>
				<td  colspan="3">
					<input type="text" name="cgjg" value="${qq.cgjg }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">折旧年限：</th>
				<td  colspan="3">
					<input type="text" name="zjnx" value="${qq.zjnx }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">保养频率：</th>
				<td  colspan="3">
					<input type="text" name="bypl" value="${qq.bypl }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">残值率：</th>
				<td  colspan="3">
					<input type="text" name="czl" value="${qq.czl }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">功率：</th>
				<td  colspan="3">
					<input type="text" name="gl" value="${qq.gl }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			
			<tr>
				<th style="width:80px">设备参数：</th>
				<td  colspan="3">
					<input type="text" name="sbcs" value="${qq.sbcs }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">责任人：</th>
				<td  colspan="3">
					<input type="text" name="zrr" value="${qq.zrr }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">设备状态：</th>
				<td  colspan="3">
					<input type="text" name="sbzt" value="${qq.sbzt }" class="easyui-validatebox" required="false" />
				</td>
			</tr>
			<tr>
				<th style="width:80px">设备类型：</th>
				<td  colspan="3">
					<select name="sblx" id="sblx" value="${qq.sblx}">
						<option value="空调">空调</option>
						<option value="暖风机">暖风机</option>
					</select>
				</td>
			</tr>
			<tr>
				<th style="width:80px">描述：</th>
				<td  colspan="3">
					<textarea rows="2" name="desc">${qq.desc }</textarea>
				</td>
			</tr>
			
			<tr>
				<th></th>
				<td colspan="3">
					<div  style="margin-top: 10px;margin-bottom: 10px;">
						  <button type="submit" class="btn btn-primary" >保存</button>&nbsp;&nbsp;&nbsp;&nbsp;
						  <button type="button" class="btn clear" >清空</button>
					</div>
				</td>
			 </tr>
			
	</table>
</form>
</div>
										
						
					
		

	

