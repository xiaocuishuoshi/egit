package com.whfp.oa.manager.app.action.model;

public class TalkCreateInputModel {


	//谈话对象的戒毒人员id
	private String toUid;
	
	//谈话对象的戒毒人员姓名
	private String toUser;
	
	//谈话地点
	private String address;
	
	//谈话内容
	private String content;
	
	//谈话者
	private String talkUid;
	
	//谈话者姓名
	private String talkUser;
	
	/**
	 * 上传的附件（多张，逗号分隔）
	 */
	private String fileIds;

	public String getToUid() {
		return toUid;
	}

	public void setToUid(String toUid) {
		this.toUid = toUid;
	}

	public String getToUser() {
		return toUser;
	}

	public void setToUser(String toUser) {
		this.toUser = toUser;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getTalkUid() {
		return talkUid;
	}

	public void setTalkUid(String talkUid) {
		this.talkUid = talkUid;
	}

	public String getTalkUser() {
		return talkUser;
	}

	public void setTalkUser(String talkUser) {
		this.talkUser = talkUser;
	}

	public String getFileIds() {
		return fileIds;
	}

	public void setFileIds(String fileIds) {
		this.fileIds = fileIds;
	}
	
	
	
}
