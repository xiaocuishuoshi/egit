<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@page import="com.whfp.oa.commons.util.ServletUtil"%>
<%@page import="com.whfp.oa.commons.model.Member"%>
<%@ include file="/WEB-INF/jsp/commons/include.inc.jsp" %>
<%--
	模块：企业宣传--公告管理 --系统公告添加
--%>
 

 
<div style="width: 98%;margin: 0 auto ;" >
			
<form method="post" action="qyzp/update.do"  onsubmit="return validateSubmitForm(this)">
	<table class="table table-bordered ">
			<input type="hidden" name="id" class="easyui-validatebox" required="true"  value="<c:out value='${qq.id }'/>"/>
			
			<tr>
				<th>公司名称：</th>
				<td >
					<input type="text" name="gsmc" value="<c:out value='${qq.gsmc }'/>" class="easyui-validatebox" required="true" />
				</td>
				<th>所属行业：</th>
				<td>
					<input type="text" name="hy" value="<c:out value='${qq.hy }'/>" class="easyui-validatebox" required="true" />
				</td>
			</tr>
			<tr>
				<th>公司性质：</th>
				<td>
					<input type="text" name="xz" value="<c:out value='${qq.xz }'/>" class="easyui-validatebox" required="true" />
				</td>
				<th>公司规模：</th>
				<td>
					<input type="text" name="gm" value="<c:out value='${qq.gm }'/>" class="easyui-validatebox" required="true" />
				</td>
			</tr>
			<tr>
				<th>薪资待遇：</th>
				<td>
					<input type="text" name="xzdy" value="<c:out value='${qq.xzdy }'/>" class="easyui-validatebox" required="true" />
				</td>
				<th>学历：</th>
				<td>
					<input type="text" name="xl" value="<c:out value='${qq.xl }'/>" class="easyui-validatebox" required="true" />
				</td>
			</tr>
			
			<tr>
				<th>招聘职位：</th>
				<td>
					<input type="text" name="zpzw" value="<c:out value='${qq.zpzw }'/>" class="easyui-validatebox" required="true" />
				</td>
				<th>工作年限：</th>
				<td>
					<input type="text" name="gznx" value="<c:out value='${qq.gznx }'/>" class="easyui-validatebox" required="true" />
				</td>
			</tr>
			<tr>
				<th>福利待遇：</th>
				<td >
					<input type="text" name="fldy" value="<c:out value='${qq.fldy }'/>" class="easyui-validatebox" required="true" />
				</td>
				<th>联系电话：</th>
				<td>
					<input type="text" name="lxdh" value="<c:out value='${qq.lxdh }'/>" class="easyui-validatebox" required="true" />
				</td>
			</tr>
			<tr>
				<th>工作地址：</th>
				<td colspan="3">
					<textarea row="2" name="gzdz"  style="width:85%;"><c:out value='${qq.lxdh }'/></textarea>
				</td>
			</tr>
			
			<tr>
				<th></th>
				<td colspan="3">
					<div  style="margin-top: 10px;margin-bottom: 10px;">
						  <button type="submit" class="btn btn-primary" >修改</button>&nbsp;&nbsp;&nbsp;&nbsp;
					</div>
				</td>
			 </tr>
			
	</table>
</form>
</div>
										
						
					
		

	

